{
  "$id": "/schemas/3.0.0-beta.2/property/property-error.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "description": "Error information for a property that could not be evaluated",
  "properties": {
    "code": {
      "description": "Error code",
      "enum": [
        "PROPERTY_NOT_FOUND",
        "PROPERTY_NOT_MONITORED",
        "LIST_NOT_FOUND",
        "LIST_ACCESS_DENIED",
        "METHODOLOGY_NOT_SUPPORTED",
        "JURISDICTION_NOT_SUPPORTED"
      ],
      "type": "string"
    },
    "message": {
      "description": "Human-readable error message",
      "type": "string"
    },
    "property": {
      "$ref": "/schemas/3.0.0-beta.2/core/property.json",
      "description": "The property that caused the error"
    }
  },
  "required": [
    "code",
    "message"
  ],
  "title": "Property Error",
  "type": "object"
}