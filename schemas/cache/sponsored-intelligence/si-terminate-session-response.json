{
  "$id": "/schemas/3.0.0-beta.2/sponsored-intelligence/si-terminate-session-response.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": true,
  "description": "Confirmation of session termination",
  "properties": {
    "acp_handoff": {
      "additionalProperties": true,
      "description": "ACP checkout handoff data (for handoff_transaction)",
      "properties": {
        "checkout_token": {
          "description": "Token for ACP checkout flow",
          "type": "string"
        },
        "checkout_url": {
          "description": "ACP checkout initiation URL",
          "format": "uri",
          "type": "string"
        },
        "product": {
          "description": "Product details for checkout",
          "type": "object"
        }
      },
      "type": "object"
    },
    "errors": {
      "items": {
        "$ref": "/schemas/3.0.0-beta.2/core/error.json"
      },
      "type": "array"
    },
    "ext": {
      "$ref": "/schemas/3.0.0-beta.2/core/ext.json"
    },
    "follow_up": {
      "additionalProperties": true,
      "description": "Suggested follow-up actions",
      "properties": {
        "action": {
          "enum": [
            "save_for_later",
            "set_reminder",
            "subscribe_updates",
            "none"
          ],
          "type": "string"
        },
        "data": {
          "description": "Data for follow-up action",
          "type": "object"
        }
      },
      "type": "object"
    },
    "session_id": {
      "description": "Terminated session identifier",
      "type": "string"
    },
    "terminated": {
      "description": "Whether session was successfully terminated",
      "type": "boolean"
    }
  },
  "required": [
    "session_id",
    "terminated"
  ],
  "title": "SI Terminate Session Response",
  "type": "object"
}